{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "esnext",
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "allowJs": true,
  "skipLibCheck": true,
  "skipPreflightCheck": true,
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "strict": true,
  "forceConsistentCasingInFileNames": true,
  "noImplicitAny": false,
  "module": "esnext",
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "noEmit": true,
  "jsx": "react",
  "baseUrl": "./src",
  "target": "es6",
  "lib": [
    "es2015",
    "dom"
  ],
  "declaration": false,
  "keyofStringsOnly": true,
  "outDir": "./dist",
  "rootDir": ".",
  "sourceMap": true,
  "include": [
    "dist"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
